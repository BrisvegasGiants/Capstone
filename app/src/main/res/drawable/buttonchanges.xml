<?xml version="1.0" encoding="utf-8"?>

<!-- Changes the buttons appearance on press -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"
        android:drawable= "@android:color/holo_blue_dark"/> <!-- pressed -->
    <item android:state_focused="true"
        android:drawable= "@android:color/holo_blue_dark"/> <!-- focused -->
    <item android:drawable="@color/colorButton" /><!-- default -->
</selector>





